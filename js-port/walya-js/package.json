{
  "name": "@tewelde/walya",
  "version": "0.1.1",
  "description": "Walya runtime for Node.js and browser environments",
  "license": "MIT",
  "type": "commonjs",
  "main": "./src/walya.js",
  "module": "./src/walya.browser.mjs",
  "browser": "./src/walya.browser.js",
  "types": "./src/walya.d.ts",
  "exports": {
    ".": {
      "types": "./src/walya.d.ts",
      "require": "./src/walya.js",
      "import": "./src/walya.browser.mjs",
      "default": "./src/walya.js"
    },
    "./browser": {
      "types": "./src/walya.browser.d.ts",
      "require": "./src/walya.browser.js",
      "import": "./src/walya.browser.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src"
  ],
  "author": "Tewelde Ma. Tegegne <teweldemat@gmail.com>",
  "keywords": [
    "expression",
    "runtime",
    "scripting",
    "walya"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teweldemat/walya.git"
  },
  "bugs": {
    "url": "https://github.com/teweldemat/walya/issues"
  },
  "homepage": "https://github.com/teweldemat/walya#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "contributors": [
    "Tewelde Ma. Tegegne <teweldemat@gmail.com>"
  ]
}

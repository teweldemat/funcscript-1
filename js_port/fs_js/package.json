{
  "name": "funcscript",
  "version": "0.0.1",
  "description": "FuncScript runtime for Node.js and browser environments",
  "license": "MIT",
  "type": "commonjs",
  "main": "./src/FuncScript.js",
  "module": "./src/FuncScript.browser.mjs",
  "browser": "./src/FuncScript.browser.js",
  "types": "./src/FuncScript.d.ts",
  "exports": {
    ".": {
      "types": "./src/FuncScript.d.ts",
      "require": "./src/FuncScript.js",
      "import": "./src/FuncScript.browser.mjs",
      "default": "./src/FuncScript.js"
    },
    "./browser": {
      "types": "./src/FuncScript.browser.d.ts",
      "require": "./src/FuncScript.browser.js",
      "import": "./src/FuncScript.browser.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src"
  ]
}
